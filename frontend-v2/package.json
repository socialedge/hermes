{
  "name": "hermes-frontend-v2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile --env dev",
    "build:prod": "rimraf dist && webpack --bail --progress --profile --env prod",
    "start": "npm-run-all --parallel serve",
    "serve": "webpack-dev-server --history-api-fallback --inline --progress --hot --env dev",
    "serve:prod": "webpack-dev-server --history-api-fallback --inline --progress --hot --env prod"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.7",
    "angular": "^1.6.0",
    "angular-animate": "^1.6.0",
    "angular-aria": "^1.6.0",
    "angular-material": "^1.1.1",
    "angular-messages": "^1.6.6",
    "angular-translate": "^2.13.0",
    "angular-translate-loader-static-files": "^2.13.0",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.7.1",
    "htmlhint": "^0.9.13",
    "json-loader": "^0.5.4",
    "minimatch": "^3.0.4",
    "ng-annotate-loader": "^0.6.1",
    "npm-run-all": "^4.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-angular-dgeni-plugin": "^0.0.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
}
